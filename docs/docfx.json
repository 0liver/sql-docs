{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml",
          "breadcrumb/toc.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
           "**/mref/**"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "ssms",
        "src": "ssms",
        "dest": "ssms",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.gif",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/*.md"
        ]
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "ssms",
        "src": "ssms",
        "dest": "ssms"
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "layout": "Conceptual",
      "breadcrumb_path": "/sql/breadcrumb/toc.json",
      "contributors_to_exclude": ["hexiaokai", "openpublishingbuild","sudeepku","saldana"],
      "searchScope": ["sql"],
      "_op_documentIdPathDepotMapping": {
        "linux/": {
          "depot_name": "SQL.linux",
          "folder_relative_path_in_docset": "."
        }
      }
    },
    "fileMetadata": {},
    "template": [],
    "dest": "docs",
    "groups": {
      "ssms": {
        "dest": "ssms",
        "moniker_range": ">= ssms-16"
      }
    },
    "xref":["mref/sql130.zip","mref/System.Reflection.Assembly.Load.yml","mref/System.Web.UI.WebControls.WebParts.IWebPartRow.yml"]
  }
}
